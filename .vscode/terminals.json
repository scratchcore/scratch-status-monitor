{
  "autorun": true,
  "autokill": true,
  "terminals": [
    {
      "name": "root",
      "cwd": "."
    },
    {
      "name": "Backend > root",
      "cwd": "./apps/backend"
    },
    {
      "name": "Backend > server",
      "cwd": "./apps/backend"
    },
    {
      "name": "Backend > cron",
      "cwd": "./apps/backend"
    },
    {
      "name": "Frontend > root",
      "cwd": "./apps/frontend"
    },
    {
      "name": "Frontend > server",
      "cwd": "./apps/frontend"
    },
    {
      "name": "pkg > configs",
      "cwd": "./packages/configs"
    },
    {
      "name": "pkg > types",
      "cwd": "./packages/types"
    },
    {
      "name": "pkg > shared-api",
      "cwd": "./packages/shared-api"
    }
  ]
}
